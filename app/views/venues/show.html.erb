<% provide(:title, @venue.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= @venue.name %>
        <br>
        <%= @venue.address %><br>
        <%= @venue.city %>,<%= @venue.state %> <%= @venue.zip %>
        <br>
        <%= @venue.phone %>
      </h1>
    </section>
  </aside>
</div>
<% if current_user.admin? %>
<%= link_to "Update Venue", edit_venue_path, class: 'btn btn-primary'%>
<% end %>
<br>
<br>
<%= link_to "Venue Index", venues_path, class: 'btn btn-primary' %>